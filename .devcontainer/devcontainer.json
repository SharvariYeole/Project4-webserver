{
  "name": "TomEE Codespace",
  "dockerFile": "Dockerfile",
  "extensions": [
    "vscjava.vscode-java-pack",
    "vsciot-vscode.vscode-arduino"
  ],
  "settings": {
    "java.home": "/usr/lib/jvm/java-17-openjdk-amd64/bin/java",
    "java.jdt.ls.java.home": "/usr/lib/jvm/java-17-openjdk-amd64/bin/java"
  },
  "forwardPorts": [8080],
  "postCreateCommand": "sudo apt update && sudo apt install -y git"
}
